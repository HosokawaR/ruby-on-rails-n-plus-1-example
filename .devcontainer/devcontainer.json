{
    "name": "Ruby on Rails N+1 Example",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "db",
    "workspaceFolder": "/workspaces/ruby-on-rails-n-plus-1-example",
    "customizations": {
        "vscode": {
            "extensions": [
                "rebornix.ruby",
                "castwide.solargraph",
                "shopify.ruby-lsp"
            ]
        }
    },
    "postCreateCommand": "bundle install && bundle exec rails db:create db:migrate",
    "remoteUser": "root"
}